{% extends 'Up2greenEducationBundle::layout.html.twig' %}

{% block content %}

{{ parent() }}

<ul class="classroom-pictures thumbnails">
    {% for picture in pictures %}
        <li class="span1">
            <a class="classroom-picture-modal" href="{{ path('education_classroom_picture_show', {'id': picture.id}) }}">
                <img class="classroom-picture img-polaroid" src="{{ asset(picture.picture) | imagine_filter('thumb_75x75_outbound')  }}" alt="{{ picture.student }}" />
            </a>
        </li>
    {% endfor %}
</ul>

{% endblock %}
