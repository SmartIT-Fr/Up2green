<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">

  <services>
    <service id="up2green_education.admin.school" class="Up2green\EducationBundle\Admin\SchoolAdmin">
      <tag name="sonata.admin" manager_type="propel" group="Education" label="Schools"/>
      <argument />
      <argument>Up2green\EducationBundle\Model\School</argument>
      <argument>SonataAdminBundle:CRUD</argument>
      <call method="setTranslationDomain">
        <argument>admin</argument>
      </call>
    </service>

    <service id="up2green_education.admin.classroom" class="Up2green\EducationBundle\Admin\ClassroomAdmin">
      <tag name="sonata.admin" manager_type="propel" group="Education" label="Classrooms"/>
      <argument />
      <argument>Up2green\EducationBundle\Model\Classroom</argument>
      <argument>SonataAdminBundle:CRUD</argument>
      <call method="setTranslationDomain">
        <argument>admin</argument>
      </call>
    </service>

    <service id="up2green_education.admin.classroom_picture" class="Up2green\EducationBundle\Admin\ClassroomPictureAdmin">
      <tag name="sonata.admin" manager_type="propel" group="Education" label="Classroom Pictures"/>
      <argument />
      <argument>Up2green\EducationBundle\Model\ClassroomPicture</argument>
      <argument>SonataAdminBundle:CRUD</argument>
      <call method="setTranslationDomain">
        <argument>admin</argument>
      </call>
    </service>

    <service id="up2green_education.admin.voucher" class="Up2green\EducationBundle\Admin\VoucherAdmin">
      <tag name="sonata.admin" manager_type="propel" group="Education" label="Vouchers"/>
      <argument />
      <argument>Up2green\EducationBundle\Model\EducationVoucher</argument>
      <argument>Up2greenEducationBundle:Admin\Voucher</argument>
      <call method="setTranslationDomain">
        <argument>admin</argument>
      </call>
    </service>
  </services>

</container>