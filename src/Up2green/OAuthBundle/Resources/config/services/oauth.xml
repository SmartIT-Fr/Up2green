<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="up2green_oauth.oauth.consumer.class">Up2green\OAuthBundle\OAuth\Consumer</parameter>
        <parameter key="up2green_oauth.oauth.method.hmac_sha1.class">Up2green\OAuthBundle\OAuth\Method\HmacSha1</parameter>
    </parameters>

    <services>
        <service id="up2green_oauth.oauth.consumer" class="%up2green_oauth.oauth.consumer.class%" />
        <service id="up2green_oauth.oauth.method.hmac_sha1" class="%up2green_oauth.oauth.method.hmac_sha1.class%" />
    </services>
</container>

