

{% for program in pager.currentPageResults %}
    <div class="item">
        <h2>{{ program.title }}</h2>
        <p>{{ program.summary |raw }}</p>
        <span class="pull-right"><a href="{{ path('blog_program_show', {'id': program.id}) }}">{{ "read_more"|trans }}</a></span>
    </div>
{% endfor %}

{% if pager.hasPreviousPage %}
<a href="#" class="previous"><<</a>
{% endif %}

{% if pager.hasNextPage %}
<a href="#" class="next">>></a>
{% endif %}
