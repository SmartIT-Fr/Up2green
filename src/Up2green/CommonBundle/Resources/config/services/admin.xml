<!-- MyBundle/Resources/config/admin.xml -->
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    
  <services>
    <service id="up2green_common.admin.voucher" class="Up2green\CommonBundle\Admin\VoucherAdmin">
      <tag name="sonata.admin" manager_type="propel" group="" label="Vouchers"/>
      <argument />
      <argument>Up2green\CommonBundle\Model\Voucher</argument>
      <argument>SonataAdminBundle:CRUD</argument>
      <call method="setTranslationDomain">
        <argument>admin</argument>
      </call>
    </service>
  </services>

</container>